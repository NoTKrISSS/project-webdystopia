{
    "manifest_version": 3,
    "name": "webdystopia",
    "version": "1.0.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "all_frames": true,
        "run_at": "document_end"
    }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "WebDystopia"
    }

}